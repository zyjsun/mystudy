.cart-box{
  padding-bottom: 100rpx;
}
.cart-list{
  padding: 20rpx 20rpx 20rpx 285rpx;
  height: 185rpx;
  border-bottom: 1px solid #e9e9e9;
  position: relative;
}
.cart-pro-select{
  position: absolute;
  left: 20rpx;
  top: 90rpx;
  width: 45rpx;
  height: 45rpx;
}
.cart-pro-goods{
  position: absolute;
  left: 85rpx;
}
.cart-thumb{
  width: 185rpx;
  height: 185rpx;
}
.cart-pro-name{
  display: inline-block;
  width: 300rpx;
  height: 105rpx;
  line-height: 50rpx;
}
.cart-pro-price{
  float: right;
  height: 105rpx;
  line-height: 50rpx;
}
.cart-count-box{
  position: absolute;
  left: 285rpx;
  bottom: 20rpx;
  width: 250rpx;
  height: 80rpx;
}
.cart-count-box text{
  display: inline-block;
  line-height: 80rpx;
  text-align: center;
}
.cart-count-down,
.cart-count-add{
  font-size: 44rpx;
  width: 50rpx;
  height: 100%;
}
.cart-count-num{
  width: 150rpx;
}
.cart-del{
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 44rpx;
}

.cart-footer{
  position: fixed;
  bottom:0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 100rpx 0 80px;
  background:#ab956d;
  box-sizing: border-box;
  color:#fff

}
.total-select{
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  width: 45rpx;
  height: 45rpx;
}